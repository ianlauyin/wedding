{
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "target": "ESNext",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "noEmit": true,
    "strict": true,
    "types": ["vite/client"],
    "baseUrl": ".",
    "paths": {
      "@components/*": ["./src/component/*"],
      "@pages/*": ["./src/pages/*"],
      "@ajax/*": ["./src/ajax/*"],
      "@util/*": ["./src/util/*"]
    }
  }
}
